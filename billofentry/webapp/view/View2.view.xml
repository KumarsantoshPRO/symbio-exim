<mvc:View
    controllerName="zpro.sk.mittalcoin.exim.bill.of.entry.billofentry.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
    xmlns:table="sap.ui.table"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:sap.ui.fl="sap.ui.fl"
>
    <uxap:ObjectPageLayout
        id="ObjectPageLayout"
        upperCaseAnchorBar="true"
        showFooter="true"
        selectedSection="idOPSHeaderDetails"
    >
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle>
                <uxap:expandedHeading>
                    <Title
                        text="{oModelForHeader>/BillEntry}"
                        wrapping="true"
                    />
                </uxap:expandedHeading>
                <uxap:snappedHeading>
                    <Title
                        text="{oModelForHeader>/BillEntry}"
                        wrapping="true"
                    />
                </uxap:snappedHeading>
                <uxap:actions>
                    <Button
                        id="idBtnEdit"
                        text="Edit"
                        press="onEditPress"
                        tooltip="Edit"
                        type="Emphasized"
                        visible="{myPropertyValues>/editButton}"
                    />
                    <Button
                        id="idBtnDelete"
                        text="Delete"
                        press="onDeletePress"
                        tooltip="Edit"
                        type="Transparent"
                        visible="{myPropertyValues>/editButton}"
                    />
                </uxap:actions>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>
        <uxap:sections>
            <uxap:ObjectPageSection
                id="idOPSHeaderDetails"
                titleUppercase="true"
                title="Bill Of Entry Header"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubHeaderDetailsView"
                        title="Bill Of Entry Header"
                        titleUppercase="true"
                        visible="{myPropertyValues>/view}"
                    >
                        <uxap:blocks>
                            <core:Fragment
                                id="idFrg_headerDetialsView"
                                fragmentName="zpro.sk.mittalcoin.exim.bill.of.entry.billofentry.view.fragments.View2.headerDetails_view"
                                type="XML"
                            />
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubHeaderDetailsEdit"
                        title="Bill Of Entry Header"
                        titleUppercase="true"
                        visible="{myPropertyValues>/edit}"
                    >
                        <uxap:blocks>
                            <core:Fragment
                                id="idFrg_headerDetialsEdit"
                                fragmentName="zpro.sk.mittalcoin.exim.bill.of.entry.billofentry.view.fragments.View2.headerDetails_edit"
                                type="XML"
                            />
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>

            <uxap:ObjectPageSection
                id="idV2OPSItemsDetail"
                showTitle="false"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubItemsDetailView"
                        title="Bill Of Entry Items"
                        titleUppercase="true"
                        visible="{myPropertyValues>/view}"
                    >
                        <uxap:blocks>
                            <core:Fragment
                                id="idFrg_itemDetailsView"
                                fragmentName="zpro.sk.mittalcoin.exim.bill.of.entry.billofentry.view.fragments.View2.itemDetails_view"
                                type="XML"
                            />
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubItemsDetailEdit"
                        title="Bill Of Entry Items"
                        titleUppercase="true"
                      
                        visible="{myPropertyValues>/edit}"
                    >
                        <uxap:blocks>
                            <core:Fragment
                                id="idFrg_itemDetailsEdit"
                                fragmentName="zpro.sk.mittalcoin.exim.bill.of.entry.billofentry.view.fragments.View2.itemDetails_edit"
                                type="XML"
                            />
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>

        <uxap:footer>
            <Bar
                id="idV2Bar"
                visible="{myPropertyValues>/footer}"
            >
                <contentRight>
                    <Button
                        id="idBtn_Save"
                        text="Save"
                        type="Emphasized"
                        press="onSaveButtonPress"
                        visible="{myPropertyValues>/update}"
                    />
                    <Button
                        id="idBtn_Create"
                        text="Create"
                        type="Emphasized"
                        press="onCreateButtonPress"
                        visible="{myPropertyValues>/new}"
                    />
                    <Button
                        id="idBtn_Cancel"
                        text="Cancel"
                        press="onCancel"
                        type="Transparent"
                    />
                </contentRight>
            </Bar>
        </uxap:footer>
    </uxap:ObjectPageLayout>
</mvc:View>
