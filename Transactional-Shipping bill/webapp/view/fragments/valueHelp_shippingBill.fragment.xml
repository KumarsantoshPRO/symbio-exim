        id="idSDShippingBillF4"
        title="Shipping Bill Number"
        noDataText="No Data"
        search="onValueHelpSearch_shippingBillNumber"
        liveChange="onValueHelpSearch_shippingBillNumber"
        confirm="onValueHelpConfirm_shippingBillNumber"
        contentWidth="250px" 
    >
        <items>
            <StandardListItem
                id="idSLShippingBillValueHelp"
                title="{ZshippingBillNo}"
               
                wrapping="true"
                
            />
